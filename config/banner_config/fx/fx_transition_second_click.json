			"animation-timing-stageOne"		: "ease-in"
		},

		"stage2" : {
			"animation-stageTwo" 		: "flipInRight",
			"transform-origin-stageTwo"		: "50% 50%",
			"animation-duration-stageTwo"	: "0.5s",
			"animation-timing-stageTwo"		: "ease-out",
			"animation-delay-stageTwo"		: "0.5s"
		}
	},

	"flip-in-bottom" :{

		"stage1" : {
			"animation-stageOne" 		: "flipOutTop",
