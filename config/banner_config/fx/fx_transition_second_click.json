{


	"cube-right" :{

		"stage1" : {
			"animation-stageOne" 		: "rotateCubeRightOut",
			"transform-origin-stageOne"		: "0% 50%",
			"animation-duration-stageOne"	: "0.6s",
			"animation-timing-stageOne"		: "ease-in"
		},

		"stage2" : {
			"animation-stageTwo" 		: "rotateCubeRightIn",
			"transform-origin-stageTwo"		: "100% 50%",
			"animation-duration-stageTwo"	: "0.6s",
			"animation-timing-stageTwo"		: "ease-in",
			"animation-delay-stageTwo"		: "0s"
		}
	},

	"cube-top" :{

		"stage1" : {
			"animation-stageOne" 		: "rotateCubeTopOut ",
			"transform-origin-stageOne"		: "50% 100%",
			"animation-duration-stageOne"	: "0.6s",
			"animation-timing-stageOne"		: "ease-in"
		},

		"stage2" : {
			"animation-stageTwo" 		: "rotateCubeTopIn",
			"transform-origin-stageTwo"		: "50% 0%",
			"animation-duration-stageTwo"	: "0.6s",
			"animation-timing-stageTwo"		: "ease-in",
			"animation-delay-stageTwo"		: "0s"
		}
	},

