{
	"appName" 			: {
						"elem" : ".details-wrapper .details-info .info-container .document-title div",
						"attribute" : "text"
					},

	"editor"				: {
						"elem" : "a[class='document-subtitle primary']",
						"attribute"  :"text"
					},

	"nbComments"			: {
						"elem"	: "meta[itemprop='ratingCount']",
						"attribute"  : "content"
					},	

	"nbStars"			: {
						"elem" : "meta[itemprop='ratingValue']",
						"attribute"  : "content"
					},
	
	"icon" 				: {
						"elem" : "img[class='cover-image']",
						"attribute" 	: "src"
					}
}